    "@testing-library/jest-dom": "^6.8.0",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "msw": "^2.11.1",
